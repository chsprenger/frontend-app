/*******************************
         Site Overrides
*******************************/

input[type='number'] {
    text-align: right;
}

input[readonly], input[readonly]:focus, input[readonly="readonly"], input[readonly="readonly"]:focus {
    background-color: #FFFFFF !important;
    box-shadow: inset 0px 0px 0px 1px rgba(34,36,38,.15) !important;
    cursor: default !important;
    color: rgba(0,0,0,.6) !important;
}