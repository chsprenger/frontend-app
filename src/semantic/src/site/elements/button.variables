@borderRadius: @defaultBorderRadius;

/* Box Shadow */
@basicBoxShadow: 0px 0px 0px @basicBorderSize @borderBoxShadowColor inset;
@basicHoverBoxShadow: @borderBoxShadowColor;

/* Basic Hover */
@basicHoverBackground: transparent;

/*-------------------
        Group
--------------------*/

/* Hovered */
@iconHoverOpacity: 1;

/* Focused */
@iconFocusOpacity: 1;